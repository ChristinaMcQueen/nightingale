{
  "extends": "eslint-config-egg",
  "parserOptions": {
    "sourceType": "module"
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "rules": {
    "indent": ["error", 4],
    "array-bracket-spacing": ["error", "never"],
    "strict": 0,
    "comma-dangle": ["error", "never"],
    "keyword-spacing": 2,
    "brace-style": [2, "stroustrup"]
  }
}
